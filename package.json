{
  "name": "@napi-rs/lzma",
  "version": "1.4.1",
  "description": "https://docs.rs/lzma-rs binding to Node.js via https://napi.rs",
  "napi": {
    "binaryName": "lzma",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "aarch64-linux-android",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "arm-linux-androideabi",
      "x86_64-unknown-linux-musl",
      "x86_64-unknown-freebsd",
      "i686-pc-windows-msvc",
      "wasm32-wasi-preview1-threads",
      "powerpc64le-unknown-linux-gnu",
      "s390x-unknown-linux-gnu",
      "riscv64gc-unknown-linux-gnu"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/Brooooooklyn/lzma.git",
    "type": "git"
  },
  "keywords": [
    "Node-API",
    "napi",
    "lzma",
    "compress",
    "decompress",
    "xz",
    "napi-rs"
  ],
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",
    "xz.js",
    "xz.d.ts",
    "lzma.js",
    "lzma.d.ts",
    "lzma2.js",
    "lzma2.d.ts",
    "browser.js"
  ],
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.62",
    "@napi-rs/wasm-runtime": "^0.2.4",
    "@oxc-node/core": "^0.0.20",
    "@taplo/cli": "^0.7.0",
    "@types/lzma-native": "^4.0.4",
    "@types/node": "^22.0.0",
    "ava": "^6.1.3",
    "conventional-changelog-cli": "^5.0.0",
    "esbuild": "^0.25.0",
    "lzma": "^2.3.2",
    "lzma-native": "^8.0.6",
    "npm-run-all2": "^7.0.0",
    "oxlint": "^0.16.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "lint": "oxlint .",
    "format": "run-p format:source format:rs format:toml",
    "format:rs": "cargo fmt",
    "format:source": "prettier --config ./package.json --write .",
    "format:toml": "taplo format",
    "prepublishOnly": "napi prepublish -t npm && esbuild --minify --outfile=index.js --allow-overwrite index.js",
    "test": "ava",
    "version": "napi version && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "ava": {
    "nodeArguments": [
      "--import",
      "@oxc-node/core/register"
    ],
    "extensions": {
      "ts": "module"
    },
    "timeout": "2m",
    "workerThreads": false,
    "environmentVariables": {
      "TS_NODE_PROJECT": "./tsconfig.json",
      "NODE_ENV": "ava"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "browser": "./browser.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts",
      "browser": {
        "import": "./browser.js"
      }
    },
    "./xz": {
      "import": "./xz.js",
      "require": "./xz.js"
    },
    "./lzma": {
      "import": "./lzma.js",
      "require": "./lzma.js"
    },
    "./lzma2": {
      "import": "./lzma2.js",
      "require": "./lzma2.js"
    }
  },
  "packageManager": "yarn@4.7.0",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  }
}
