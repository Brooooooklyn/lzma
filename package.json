{
  "name": "@napi-rs/lzma",
  "version": "0.0.0",
  "description": "https://docs.rs/lzma-rs binding to Node.js via https://napi.rs",
  "napi": {
    "name": "lzma",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "Node-API",
    "napi",
    "lzma",
    "compress",
    "decompress",
    "xz",
    "napi-rs"
  ],
  "license": "MIT",
  "files": [
    "index.js",
    "xz.js",
    "xz.d.ts",
    "lzma.js",
    "lzma.d.ts",
    "lzma2.js",
    "lzma2.d.ts"
  ],
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^1.3.1",
    "@node-rs/deno-lint": "^1.9.2",
    "@swc-node/register": "^1.3.5",
    "@types/lzma-native": "^4.0.1",
    "@types/node": "^16.9.1",
    "ava": "^3.15.0",
    "conventional-changelog-cli": "^2.1.1",
    "lzma": "^2.3.2",
    "lzma-native": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "lint": "denolint .",
    "format": "run-p format:md format:json format:yaml format:source format:rs",
    "format:md": "prettier --parser markdown --write ./**/*.md",
    "format:json": "prettier --parser json --write ./**/*.json",
    "format:rs": "cargo fmt",
    "format:source": "prettier --config ./package.json --write ./**/*.{js,ts}",
    "format:yaml": "prettier --parser yaml --write ./**/*.{yml,yaml}",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "ava": {
    "require": [
      "@swc-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "timeout": "30s",
    "environmentVariables": {
      "SWC_NODE_PROJECT": "./tsconfig.json",
      "NODE_ENV": "ava"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  }
}